{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    
    "rewrites":[
    {
    "src": "/(.*)",
    "dest": "index.js"
    },
    {
        "src": "/(.+)",
        "dest": "build/$1"
        }
]

}


